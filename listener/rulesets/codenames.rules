spell number for => 'four '^
spell spell => 'spell '^
spell number two => 'two '^
spell t o => 'to '^

not equal => '!='
double equal => '=='
double equals => '=='
asterisk asterisk  => '**'^
star star => ^'**'^
arrow symbol => '=>'
divided by => "/"
times equal  => "*="
times => "*"
